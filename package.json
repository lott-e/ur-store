{
  "name": "medsy",
  "version": "1.4.0",
  "private": true,
  "author": "RedQ, Inc.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "yarn workspaces run rimraf \"{.next,dist,build,node_modules}\" && rimraf node_modules",
    "dev:medsy-modern": "yarn workspace @medsy/modern dev",
    "build:medsy-modern": "yarn workspace @medsy/modern build",
    "serve:medsy-modern": "yarn workspace @medsy/modern start",
    "deploy-vercel:medsy-modern": "cd packages/medsy-modern && vercel --prod",
    "prepare": "husky install"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,md,css}": [
      "prettier --trailing-comma es5 --single-quote --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@react-three/fiber": "^7.0.7",
    "@vercel/next": "^2.7.9",
    "@vercel/node": "^1.12.1",
    "@vercel/static-build": "^0.18.0",
    "airtable": "^0.11.1",
    "konva": "^8.1.4",
    "next": "^11.1.2",
    "next-images": "^1.8.1",
    "next-optimized-images": "^2.6.2",
    "react": "^17.0.2",
    "react-konva": "^17.0.2-5",
    "react-native": "^0.65.1",
    "react-zdog": "^1.0.11",
    "three": "^0.132.2",
    "url-loader": "^4.1.1",
    "vercel": "^23.1.2",
    "web-vitals": "^2.1.0",
    "webpack": "^5.53.0",
    "zdog": "^1.1.2"
  },
  "main": "index.js",
  "repository": "https://github.com/newtcandoit/ur-store",
  "license": "MIT"
}
